{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "outputDirectory": ".next",
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "framework": "nextjs",
    "routes": [
      { "src": "/blog/(.*)", "dest": "/blog/[slug].js" }
    ]
}